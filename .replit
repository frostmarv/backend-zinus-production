modules = ["nodejs-20"]
[agent]
expertMode = true
integrations = ["javascript_database:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "NestJS Server"

[[workflows.workflow]]
name = "NestJS Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run start:dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 38453
externalPort = 3000

[deployment]
deploymentTarget = "vm"
run = ["npm", "run", "start:prod"]
build = ["npm", "run", "build"]
