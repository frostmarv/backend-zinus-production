{
  "name": "backend-zinus-production",
  "version": "0.0.1",
  "description": "Backend for Zinus Production with Google Sheets integration",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "npx rimraf dist *.tsbuildinfo",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "postbuild": "mkdir -p dist/config && cp src/config/*.json dist/config/",
    "prestart": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "node --max-old-space-size=2048 ./node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:debug": "node --max-old-space-size=2048 --inspect-brk ./node_modules/@nestjs/cli/bin/nest.js start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "verify:build": "node scripts/verify-build.js",
    "deploy:check": "npm run build && npm run verify:build",
    "env:switch": "node scripts/switch-env.js",
    "env:dev": "node scripts/switch-env.js dev",
    "env:local-pg": "node scripts/switch-env.js local-pg",
    "env:prod": "node scripts/switch-env.js prod",
    "env:docker": "node scripts/switch-env.js docker",
    "dev:sqlite": "npm run env:dev && npm run start:dev",
    "dev:postgres": "npm run env:local-pg && npm run start:dev",
    "replit:dev": "npm run start:dev",
    "replit:build": "npm run build",
    "replit:start": "./start.sh",
    "replit:prod": "npm run build && npm run start",
    "replit:simple": "npm run build && node dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cron": "^4.3.3",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.2",
    "firebase-admin": "^13.5.0",
    "form-data": "^4.0.4",
    "googleapis": "^160.0.0",
    "handlebars": "^4.7.8",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@types/xlsx": "^0.0.35",
    "copyfiles": "^2.4.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
