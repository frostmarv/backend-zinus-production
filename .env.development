# For local development with SQLite

NODE_ENV=development
DB_TYPE=sqlite
SQLITE_PATH=dev.sqlite
DB_SYNC=true
DB_LOGGING=true
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://127.0.0.1:3000
PORT=5000

# Optional: Enable detailed logging
LOG_LEVEL=debug